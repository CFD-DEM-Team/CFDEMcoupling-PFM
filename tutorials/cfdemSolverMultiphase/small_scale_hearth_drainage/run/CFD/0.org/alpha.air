FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    walls
    {
        type            alphaContactAngle;
        thetaProperties
        (
            ( iron slag ) 90 0 0 0
            ( iron air ) 90 0 0 0
            ( slag air ) 90 0 0 0

        );
        value           uniform 0;
    }
    atmosphere
    {
		type		fixedValue;
		value		uniform 1;
    }
    outlet
    {
        type		zeroGradient;
    }
 
}


