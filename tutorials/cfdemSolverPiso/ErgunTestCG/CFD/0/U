FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{

    wall
    {
        //type            fixedValue;
        //value           uniform (0 0 0);
        type            slip;
    }

    inlet
    {
        /*type            flowRateInletVelocity;
        flowRate        0.001;
        value           uniform (0 0 0);*/

        /*type            fixedValue;
        value           uniform (0 0 0.0001);*/

        //type            zeroGradient;

/*        type            groovyBC;
        variables       "Uend=vector(0,0,0.02);tEnd=0.1;";
        valueExpression "((time() < tEnd) ? Uend/tEnd*time():Uend)";
        value           uniform (0 0 0);*/

        /*// 2.0.x, 1.6,ext
        type            timeVaryingUniformFixedValue;
        fileName        "steps_0p1s";
        outOfBounds     clamp;
        value           uniform (0 0 0);*/

        // 2.1.x
        type            uniformFixedValueVoidfraction;
        uniformValue     table
        (
            (0.000 (0 0 0.002))
            (0.020 (0 0 0.002))
            (0.021 (0 0 0.004))
            (0.040 (0 0 0.004))
            (0.041 (0 0 0.006))
            (0.060 (0 0 0.006))
            (0.061 (0 0 0.008))
            (0.080 (0 0 0.008))
            (0.081 (0 0 0.010))
            (0.100 (0 0 0.010))
            (0.101 (0 0 0.012))
            (0.120 (0 0 0.012))
            (0.121 (0 0 0.014))
            (0.140 (0 0 0.014))
            (0.141 (0 0 0.016))
            (0.160 (0 0 0.016))
            (0.161 (0 0 0.018))
            (0.180 (0 0 0.018))
            (0.181 (0 0 0.020))
            (0.200 (0 0 0.020))
        );
    }

    outlet
    {
        /*type            fluxCorrectedVelocity; //inletOutlet;
        value           uniform (0 0 0);
        inletValue      uniform (0 0 0);*/
        type            zeroGradient;
    }
}

