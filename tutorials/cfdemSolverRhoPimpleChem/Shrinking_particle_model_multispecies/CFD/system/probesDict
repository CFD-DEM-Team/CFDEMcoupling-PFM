FoamFile
{
	version		2.0;
	format		ascii;
	class		dictionary;
	object		probesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //



fields
(
    rho
    p
    T
    molarConc
    O2
    O
    CO2
    CO
    ModSpeciesMassField_O2
    ModSpeciesMassField_CO2
    ModSpeciesMassField_CO
);

writeControl   timeStep;
writeInterval  10;


// Locations to be probed.
probeLocations
(
	(7.0 2.5 2.5)
        (8.0 2.5 2.5)
        (1.0 2.5 2.5)
);

