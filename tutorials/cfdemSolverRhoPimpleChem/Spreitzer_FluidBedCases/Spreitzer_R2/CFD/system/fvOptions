FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
limitedTemperature
{
    type        limitTemperature;
    active      yes;
    limitTemperatureCoeffs
    {
        active          yes;
        selectionMode   all;
        Tmin     1020;
        Tmax     2000;
    }
} 

